{
  "name": "sirupsen.github.io",
  "trailingSlash": true,
  "cleanUrls": true,
  "redirects": [
    { "source": "/brief-bits-podcast/", "destination": "https://feeds.transistor.fm/brief-bits", "statusCode": 302 }
  ],
  "headers": [
    {
      "source": "/static/images/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "max-age=604800, s-maxage=604800, stale-while-revalidate, public"
        }
      ]
    }
  ]
}
